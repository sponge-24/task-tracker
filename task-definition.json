{
    "family": "task-tracker-task",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "task-tracker-container",
            "image": "IMAGE_URL_PLACEHOLDER",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
